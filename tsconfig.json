{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "declaration": true,
      "declarationDir": "dist",
      "emitDeclarationOnly": false,
      "outDir": "dist",
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "importHelpers": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }